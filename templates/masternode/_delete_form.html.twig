<form method="post" action="{{ path('masternode_delete', {'id': masternode.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ masternode.id) }}">
    <button class="btn btn-danger install-button">Delete Masternode</button>
</form>
